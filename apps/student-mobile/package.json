{
  "name": "student-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "start:devclient": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "expo export --platform ios,android",
    "build:dev:android": "eas build -p android --profile development",
    "build:dev:ios": "eas build -p ios --profile development",
    "build:preview:android": "eas build -p android --profile preview",
    "build:preview:ios": "eas build -p ios --profile preview",
    "build:prod:android": "eas build -p android --profile production",
    "build:prod:ios": "eas build -p ios --profile production",
    "submit:ios": "eas submit -p ios --latest",
    "submit:android": "eas submit -p android --latest",
    "update:preview": "eas update --channel preview",
    "update:prod": "eas update --channel production",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .expo dist"
  },
  "dependencies": {
    "@boatacademy/api-client": "workspace:*",
    "@boatacademy/shared": "workspace:*",
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@shopify/flash-list": "^2.0.2",
    "@supabase/supabase-js": "^2.47.10",
    "@tanstack/react-query": "^5.62.11",
    "expo": "~54.0.31",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.2",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "whatwg-fetch": "^3.6.20",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@boatacademy/config": "workspace:*",
    "@types/react": "~19.1.17",
    "eslint": "^9.17.0",
    "nativewind": "^4.1.23",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
